package com.zelda;

import com.badlogic.gdx.graphics.Texture;
import com.badlogic.gdx.graphics.g2d.Sprite;

public class JoueurInter {
	Sprite sprite;
	Texture linkTexture;
	private float linkX;
	private float linkY;
	private float linkSpeed;
	
	public JoueurInter(Sprite sprite, Texture linkTexture, float linkX, float linkY, float linkSpeed) {
		super();
		this.sprite = sprite;
		this.linkTexture = linkTexture;
		this.linkX = linkX;
		this.linkY = linkY;
		this.linkSpeed = linkSpeed;
	}

	public JoueurInter() {

	}
	
	public void create () {
		this.sprite= new Sprite( new Texture ("link.png"));
	}
	
}
